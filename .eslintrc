{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "curly": 0,
    "quotes": [2, "single"],
    "space-unary-ops": 0,
    "no-underscore-dangle": 0,
    "no-mixed-requires": 0,
    "no-use-before-define": 0
  }
}
